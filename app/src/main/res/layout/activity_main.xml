<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dedy.infodedy.MainActivity"
    tools:ignore="GradleDependency">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="392dp"
        android:layout_height="264dp"
        app:srcCompat="@drawable/gue"
        tools:ignore="ContentDescription"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/TentangSaya"
        android:layout_width="161dp"
        android:layout_height="46dp"
        android:text="Tentang Saya"
        android:textColor="@android:color/holo_blue_light"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.564" />

    <TextView
        android:id="@+id/txNama"
        android:layout_width="321dp"
        android:layout_height="27dp"
        android:text="Nama       : Dedy Dwi Arfiyanto"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.681"
        app:layout_constraintLeft_toLeftOf="@+id/txTtl" />

    <TextView
        android:id="@+id/txTtl"
        android:layout_width="314dp"
        android:layout_height="28dp"
        android:text="TTL          : Jepara, 22 februari 1997"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.739"
        app:layout_constraintLeft_toLeftOf="@+id/txStatus" />

    <TextView
        android:id="@+id/txStatus"
        android:layout_width="290dp"
        android:layout_height="30dp"
        android:text="Status      : Mahasiswa UNISNU"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/txProdi"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/txProdi"
        android:layout_width="300dp"
        android:layout_height="27dp"
        android:text="Prodi        : Teknik Informatika"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/txNim"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.857" />

    <TextView
        android:id="@+id/txNim"
        android:layout_width="248dp"
        android:layout_height="27dp"
        android:text="Nim           : 141240000282"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.913"
        tools:ignore="HardcodedText"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.205" />

</android.support.constraint.ConstraintLayout>
